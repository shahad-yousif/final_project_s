<p id="notice"><%= notice %></p>
<%= render 'homenave' %>
<div class="first">
<h1>Airports</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @airportlocs.each do |airportloc| %>
      <tr>
        <td><%= airportloc.address %></td>
        <td><%= airportloc.latitude %></td>
        <td><%= airportloc.longitude %></td>
        <td><%= link_to 'Show', airportloc %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

</div>


